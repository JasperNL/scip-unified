#--- $Id: make.linux.x86.insure.dbg,v 1.7.2.1 2009/06/19 07:53:34 bzfwolte Exp $
CC		=	insure gcc
CXX		=	insure g++
FLAGS		+=	# -DROUNDING_FE
OFLAGS		+=	-O0 -g
CFLAGS		+=	$(GCCWARN)
CXXFLAGS	+=	$(GXXWARN)
ARFLAGS		=	crs
LDFLAGS		+=	-Wl,-rpath,$(SCIPDIR)/$(LIBDIR)
ZLIB_FLAGS	=
ZLIB_LDFLAGS 	=	-lz
GMP_FLAGS	=
GMP_LDFLAGS 	=	-lgmp
READLINE_FLAGS	=
READLINE_LDFLAGS=	-lreadline -lncurses
