#--- $Id: make.linux.x86_64.intel.opt,v 1.1.2.3 2010/03/22 16:05:08 bzfwolte Exp $
CC		=	icc
CXX		=	icpc
LINKCC		=	icc
LINKCXX		=	icpc
FLAGS		+=	-DNDEBUG -DROUNDING_FE
OFLAGS		+=	-O3 -axSSE4.1 -w1 -vec_report0 -par_report0 -ip -mtune=core2 #-msse4.1 #-fno-rtti
CFLAGS		+=
CXXFLAGS	+=
ARFLAGS		=	crs
LDFLAGS		+=	-lpthread -lm -lstdc++ -static-intel
ZLIB_FLAGS	=
ZLIB_LDFLAGS 	=	-lz
GMP_FLAGS	=
GMP_LDFLAGS 	=	-lgmp
READLINE_FLAGS	=
READLINE_LDFLAGS=	-lreadline -lncurses
